{
  "name": "@atelier-wb/example-sveltekit",
  "version": "0.0.1",
  "description": "Example of Atelier for plain Svelte projects",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "atelier": "vite build --mode export-atelier",
    "atelier:preview": "serve dist-atelier",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "dev": "vite dev --open",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@atelier-wb/vite-plugin-atelier": "workspace:*",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.27.6",
    "serve": "^14.2.1",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.4"
  },
  "dependencies": {
    "format-message": "^6.2.4",
    "svelte-intl": "^1.1.4"
  }
}
